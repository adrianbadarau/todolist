<h1><%= @todolist.title %></h1>

<ul class="todo_items">
  <% @todolist.todo_items.each do |todo_item| %>
    <li id="<%= dom_id(todo_item) %>">
      <%= todo_item.content %>
      <%= link_to 'Edit', edit_todolist_todo_item_path(todo_item) %>
    </li>
  <% end %>
</ul>

<p><%= link_to 'New Todo Item', new_todolist_todo_item_path %></p>